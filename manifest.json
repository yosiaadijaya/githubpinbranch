{
    "name": "Github Pin Branch",
    "version": "0.1",
    "description": "Pin branch in compare page",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_popup": "options.html"
    }
}